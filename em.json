{
  "name": "EmeraldsString",

  "version": "0.1.0",

  "dependencies": {
    "EmeraldsBool": "The-Fourth-Wall/EmeraldsBool",
    "EmeraldsPreprocessor": "The-Fourth-Wall/EmeraldsPreprocessor",
    "EmeraldsVector": "The-Fourth-Wall/EmeraldsVector"
  },

  "dev-dependencies": {
    "cSpec": "Oblivious-Oblivious/cSpec"
  },

  "build": "",

  "compile-flags": {
    "cc": "clang",
    "debug": {
      "opt": "-Og -g",
      "version": "-std=c11",
      "flags": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic",
      "warnings": "-Wno-extra-semi -Wno-int-conversion -Wno-incompatible-pointer-types -Wno-unused-function",
      "libs": ""
    },
    "release": {
      "opt": "-O2",
      "version": "-std=c2x",
      "flags": "",
      "warnings": "-Wno-extra-semi -Wno-int-conversion",
      "libs": ""
    },
    "test": {
      "opt": "-Og -g",
      "version": "-std=c11",
      "flags": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic",
      "warnings": "-Wno-int-conversion -Wno-implicit-function-declaration -Wno-incompatible-pointer-types -Wno-unused-function -Wno-extra-semi",
      "libs": ""
    }
  },

  "license": "gpl-v3"
}
