{
  "name": "EmeraldsString",

  "version": "0.1.0",

  "dependencies": {
    "EmeraldsPreprocessor": "The-Fourth-Wall/EmeraldsPreprocessor",
    "EmeraldsVector": "The-Fourth-Wall/EmeraldsVector"
  },

  "dev-dependencies": {
    "cSpec": "Oblivious-Oblivious/cSpec"
  },

  "build": "",

  "compile-flags": {
    "cc": "clang",
    "debug": {
      "opt": "-Og",
      "version": "-std=c11",
      "flags": "-g -fsanitize=address",
      "warnings": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic"
    },
    "release": {
      "opt": "-O2",
      "version": "-std=c2x",
      "flags": "",
      "warnings": "-Wno-extra-semi -Wno-int-conversion"
    }
  },

  "license": "gpl-v3"
}
