{
  "name": "EmeraldsString",

  "version": "0.1.0",

  "dependencies": {
    "EmeraldsBool": "The-Fourth-Wall/EmeraldsBool",
    "EmeraldsPreprocessor": "The-Fourth-Wall/EmeraldsPreprocessor",
    "EmeraldsVector": "The-Fourth-Wall/EmeraldsVector"
  },

  "dev-dependencies": {
    "cSpec": "Oblivious-Oblivious/cSpec"
  },

  "build": "",

  "compile-flags": {
    "cc": "clang",
    "debug": {
      "opt": "-Og",
      "version": "-std=c11",
      "flags": "-g",
      "warnings": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic -Wno-extra-semi -Wno-int-conversion -Wno-incompatible-pointer-types -Wno-unused-function"
    },
    "release": {
      "opt": "-O2",
      "version": "-std=c2x",
      "flags": "",
      "warnings": "-Wno-extra-semi -Wno-int-conversion"
    },
    "test": {
      "opt": "-Og",
      "version": "-std=c11",
      "flags": "-g",
      "warnings": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic -Wno-extra-semi -Wno-unused-function"
    }
  },

  "license": "gpl-v3"
}
